{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.2",
        "drupal/allowed_formats": "^2.0",
        "drupal/authorization": "^1.0",
        "drupal/bartik": "^1.0",
        "drupal/better_social_sharing_buttons": "^4.0",
        "drupal/block_attributes": "^2.0",
        "drupal/block_visibility_groups": "^2.0",
        "drupal/bootstrap5": "^4.0",
        "drupal/ckeditor": "^1.0",
        "drupal/coder": "^8.3",
        "drupal/color": "^1.0",
        "drupal/composer_deploy": "^1.7",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/contact_block": "^2.0",
        "drupal/contact_storage": "^1.2",
        "drupal/core-composer-scaffold": "10.3.5",
        "drupal/core-project-message": "10.3.5",
        "drupal/core-recommended": "10.3.5",
        "drupal/devel": "^5.1",
        "drupal/devel_entity_updates": "^4.2",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_print": "^2.7",
        "drupal/entity_update": "^3.0",
        "drupal/externalauth": "^2.0",
        "drupal/faqfield": "^7.0",
        "drupal/field_group": "^3.3",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_lb": "^1.0@RC",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/imce": "^3.1",
        "drupal/ldap": "^4.12",
        "drupal/libraries": "^4.0",
        "drupal/md_slider": "^1.5",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/paragraphs": "^1.18",
        "drupal/pathauto": "^1.10",
        "drupal/php": "^1.2",
        "drupal/plupload": "^2.1",
        "drupal/poll": "^2.0@alpha",
        "drupal/printable": "^3.1",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.1",
        "drupal/redirect_after_login": "^3.0",
        "drupal/require_login": "^3.0",
        "drupal/seven": "^1.0",
        "drupal/sharethis": "^3.0",
        "drupal/simplenews": "^4.0",
        "drupal/social_simple": "^2.0",
        "drupal/stable": "^2.1",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/tablefield": "^2.3",
        "drupal/ultimate_cron": "2.0-alpha7",
        "drupal/upgrade_status": "^4.3",
        "drupal/views_autocomplete_filters": "^1.7",
        "drupal/webform": "^6.1",
        "drush/drush": "*",
        "mglaman/composer-drupal-lenient": "^1.0",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony-cmf/routing": "^3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
               "Translation error when installing modules" : "https://www.drupal.org/files/issues/2024-10-10/%20TranslatableMarkupError.patch"
            },
            "drupal/image_raw_formatter": {
                "Automated Drupal 10 compatibility fixes" :"https://www.drupal.org/files/issues/2023-06-10/image_raw_formatter-d10_update-3297407-4.patch"
            },
            "drupal/php" :{
                "update module php" : "patches/phpmodule.patch"
            }
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/field_collection",
                "drupal/token",
                "drupal/image_raw_formatter",
                "drupal/externalauth",
                "drupal/printfriendly"
            ]
        }
    }
}
